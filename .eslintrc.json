{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": [
   "eslint:recommended",
   "plugin:prettier/recommended",
   "plugin:react/recommended",
   "prettier/react"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack/dev.config.js"
      }
    }
  },
  "rules": {
    "semi": "off",
    "no-console": 1
  }
}
